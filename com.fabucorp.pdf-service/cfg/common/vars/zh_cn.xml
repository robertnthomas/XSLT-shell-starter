  <variable id="Table of Contents Part">第<param ref-name="number"/>部分</variable>
  <variable id="Unordered List square bullet">&#x25A0;</variable>
  <variable id="Unordered List bullet 2">—</variable>

  <!-- Image path to use for a note of "notice" type. -->
  <variable id="notice Note Image Path"/>
  <!-- Image path to use for a note of "trouble" type. -->
  <variable id="trouble Note Image Path"/>
  <!-- Image path to use for a note of "caution" type. -->
  <variable id="caution Note Image Path"
    >Customization/OpenTopic/common/artwork/caution_yellow.svg</variable>
  <!-- Image path to use for a note of "danger" type. -->
  <variable id="danger Note Image Path"
    >Customization/OpenTopic/common/artwork/caution_red.svg</variable>
  <!-- Image path to use for a note of "warning" type. -->
  <variable id="warning Note Image Path"
    >Customization/OpenTopic/common/artwork/caution_orange.svg</variable>
  <variable id="caution Note Image Path ANSI"
    >Customization/OpenTopic/common/artwork/caution_black.svg</variable>
  <!-- Image path to use for a note of "danger" type. -->
  <variable id="danger Note Image Path ANSI"
    >Customization/OpenTopic/common/artwork/caution_white.svg</variable>
  <!-- Image path to use for a note of "warning" type. -->
  <variable id="warning Note Image Path ANSI"
    >Customization/OpenTopic/common/artwork/caution_black.svg</variable>
  
  <variable id="On the page">（在第&#xa0;<param ref-name="pagenum"/>&#xa0;页上）</variable>
  
  <!-- Template for generated page number of a reference
         in printed manual, when referenced element has no title. -->
  <variable id="Page"> 第<param ref-name="pagenum"/>页</variable>
  
